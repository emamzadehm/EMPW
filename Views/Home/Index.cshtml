<section class="section section-home section-home-2 " id="home">

    <div id="scene">
        <img data-depth="0.1" class="shape1" src="~/assets/images/decor1.png" alt="Scene Images1">
        <img data-depth="0.1" class="shape4" src="~/assets/images/decor4.png" alt="Scene Images1">
        <img data-depth="0.1" class="shape5" src="~/assets/images/shape5.svg" alt="Scene Images1">
